<p>Form errors</p>
<% if form_data.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains <%= pluralize(form_data.errors.count, "error") %>.
    </div>
    <ul>
    <% form_data.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
