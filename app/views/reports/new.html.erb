
<div class="row">
    <h1>Add a New Report</h1>

    <%= form_for(@report) do |f| %>

    <div class="form-group">
        <%= f.label :website_id %>
        <%= f.collection_select(:website_id, Website.all, :id, :name, {}, {:class=>'form-control'}) %>
    </div>
    <div class="form-group">
        <%= f.label :profile_id %>
        <%= f.collection_select(:profile_id, Profile.all, :id, :name, {}, {:class=>'form-control'}) %>
    </div>
    <div class="form-group">
        <%= f.label :wpt_id %>
        <%= f.text_field :wpt_id, class: "form-control" %>
    </div>

      <%= f.submit "Save Report Data", class: "btn btn-primary" %>

    <% end %>
</div>
